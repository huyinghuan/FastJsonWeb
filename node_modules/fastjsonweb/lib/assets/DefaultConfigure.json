{
	"sessiontimeout" : 1800000,
	"static" : "public",
	"smartrouter":true,
  "ViewEngine":{
      "jade":{
         "path":"view",
         "suffix":".jade"
      },
      "ejs":{
          "path":"view",
          "suffix":".ejs"
      }
  },
  "filter":{
    "path":"",
    "sequence":[]
  },
  "log":{
    "path":"log",
    "logDateformat":"yyyy-MM-dd hh:mm:ss",
    "fileDateformat":"yyyy-MM-dd",
    "method":"log2terminal",
    "logRate":false,
    "headerInfo":{
      "time":false,
      "level":true,
      "stack":true
    }
  }
}